pipeline {
   agent {
       label 'slave'
   }
   tools {
     maven "maven"  
   }
   stages {
      stage('Git Checkout') {
         steps {
            git 'https://github.com/Chandandhani/loginapp.git'   
         }
      }
    
    stage ('Maven Goal'){
      steps {
         sh "mvn clean install package    " 
      }    
    }


    }
}
